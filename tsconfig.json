{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"outDir": "build",
		"forceConsistentCasingInFileNames": false,
		"jsx": "react",
		"types": [
			"@modyfi/vite-plugin-yaml/modules"
		]
	}
}
